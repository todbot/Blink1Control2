{
  "name": "Blink1Control2",
  "version": "2.2.9",
  "author": "ThingM <blink1@thingm.com> (https://blink1.thingm.com/)",
  "description": "Blink1Control2 GUI app for blink(1) devices",
  "keywords": [
    "blink(1)",
    "blink1",
    "node-hid",
    "node-blink1",
    "usb",
    "hid",
    "notification",
    "rgb",
    "led",
    "electron",
    "react"
  ],
  "productName": "Blink1Control2",
  "companyName": "ThingM",
  "homepage": "https://github.com/todbot/Blink1Control2",
  "repository": "https://github.com/todbot/Blink1Control2",
  "bugs": "https://github.com/todbot/Blink1Control2/issues",
  "license": "ISC",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "clean": "rimraf node_modules package-lock.json yarn.lock yarn-error.log"
  },
  "electronWebpack": {
    "renderer": {
      "webpackConfig": "webpack.renderer.additions.js"
    },
    "main": {
      "webpackConfig": "webpack.main.additions.js"
    }
  },
  "build": {
    "appId": "com.todbot.blink1control2",
    "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
    "afterSign": "pkg/notarize.js",
    "mac": {
      "target": "zip",
      "category": "public.app-category.utilities",
      "icon": "pkg/icon.icns",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "./pkg/entitlements.mac.inherit.plist",
      "entitlementsInherit": "./pkg/entitlements.mac.inherit.plist"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "build/icon.ico"
    },
    "linux": {
      "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
      "category": "Utility",
      "target": [
        "tar.gz",
        "deb",
        "appimage"
      ]
    }
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "create-react-class": "^15.7.0",
    "d3-timer": "^1.0.10",
    "electron-is-accelerator": "^0.2.0",
    "electron-log": "^4.4.1",
    "electron-root-path": "^1.0.16",
    "electron-updater": "^4.3.9",
    "events": "^1.1.1",
    "express": "^4.16.4",
    "font-awesome": "^4.7.0",
    "imap": "^0.8.19",
    "is-electron-renderer": "^2.0.1",
    "mqtt": "^4.2.1",
    "nconf": "^0.10.0",
    "needle": "^2.5.2",
    "node-blink1": "^0.5.1",
    "react": "^16.14.0",
    "react-bootstrap": "^0.33.1",
    "react-bootstrap-switch": "^15.5.3",
    "react-dom": "^16.14.0",
    "simplecrypt": "^0.1.0",
    "source-map-support": "^0.5.19",
    "tinycolor2": "^1.4.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "electron": "11.4.12",
    "electron-builder": "^22.4.1",
    "electron-notarize": "^1.1.0",
    "electron-webpack": "^2.8.2",
    "webpack": "4.46.0"
  }
}
